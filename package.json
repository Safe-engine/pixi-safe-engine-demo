{
  "name": "pixi-safe-engine-demo",
  "version": "2.0.3",
  "description": "show case and code examples",
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "start": "parcel src/index.html -p 8089 --no-cache",
    "sync-res": "bun ./node_modules/safex-sync-res/src/index.ts",
    "predev": "bun run sync-res",
    "dev": "parcel src/index.html -p 8090",
    "build": "parcel build src/index.html --reporter @parcel/reporter-bundle-analyzer",
    "fix": "eslint ./src --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@safe-engine/cocos": "workspace:*",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@parcel/config-default": "^2.15.4",
    "@parcel/reporter-bundle-analyzer": "^2.15.4",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "babel-plugin-safe-x-transform": "workspace:*",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-lodash-import-method": "^0.0.3",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "parcel": "^2.15.4",
    "safex-sync-res": "^1.2.3",
    "typescript": "^5.8.3"
  }
}